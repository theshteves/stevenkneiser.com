{
  "name": "steven-kneiser",
  "version": "1.1.0",
  "private": true,
  "description": "Steven Kneiser's little home on the internet",
  "author": "theshteves",
  "scripts": {
    "dev": "gatsby develop -p 4000 -- --https",
    "build": "gatsby build",
    "serve": "gatsby serve -p 4000 -- --https",
    "clean": "gatsby clean",
    "test": "cypress open",
    "test:mobile": "cypress open --config viewportWidth=375,viewportHeight=667",
    "lint": "eslint src cypress --ext .js,.jsx --ignore-pattern '**/examples/*'",
    "lint:fix": "npm run lint -- --fix",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:css:fix": "npm run lint:css -- --fix",
    "refresh": "curl -X POST http://localhost:4000/__refresh",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "sb": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "chromatic --exit-zero-on-changes"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "gatsby": "^5.12.12",
    "gatsby-plugin-advanced-sitemap": "^2.1.0",
    "gatsby-plugin-feed": "^5.12.12",
    "gatsby-plugin-google-tagmanager": "^5.12.12",
    "gatsby-plugin-image": "^3.12.12",
    "gatsby-plugin-manifest": "^5.12.12",
    "gatsby-plugin-mdx": "^5.12.12",
    "gatsby-plugin-postcss": "^6.12.12",
    "gatsby-plugin-react-helmet": "^6.12.12",
    "gatsby-plugin-sharp": "^5.12.12",
    "gatsby-remark-autolink-headers": "^6.12.12",
    "gatsby-remark-images": "^7.12.12",
    "gatsby-source-filesystem": "^5.12.12",
    "gatsby-transformer-remark": "^6.12.12",
    "gatsby-transformer-sharp": "^5.12.12",
    "postcss": "^8.5.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-calendly": "^4.3.1",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@mdx-js/mdx": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@storybook/addon-actions": "^8.6.8",
    "@storybook/addon-docs": "^8.6.8",
    "@storybook/addon-essentials": "^8.6.8",
    "@storybook/addon-links": "^8.6.8",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^8.6.8",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^8.6.8",
    "autoprefixer": "^10.4.21",
    "axios": "^1.8.4",
    "babel-loader": "^10.0.0",
    "chromatic": "^11.27.0",
    "cypress": "^14.2.0",
    "eslint": "^9.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-chai-friendly": "^1.0.1",
    "eslint-plugin-cypress": "^4.2.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "gatsby-plugin-page-progress": "^2.2.1",
    "gh-pages": "^6.3.0",
    "react-spring": "^9.7.5",
    "storybook-addon-gatsby": "^0.0.5",
    "stylelint": "^16.16.0",
    "stylelint-config-standard": "^37.0.0",
    "tailwindcss": "^3.3.5",
    "xml2js": "^0.6.2"
  }
}
