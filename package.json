{
  "name": "steven-kneiser",
  "version": "1.0.0",
  "private": true,
  "description": "Steven Kneiser's little home on the internet",
  "author": "theshteves",
  "scripts": {
    "dev": "gatsby develop -p 4000 -- --https",
    "build": "gatsby build",
    "serve": "gatsby serve -p 4000 -- --https",
    "clean": "gatsby clean",
    "test": "cypress open",
    "test:mobile": "cypress open --config viewportWidth=375,viewportHeight=667",
    "lint": "eslint src cypress --ext .js,.jsx --ignore-pattern '**/examples/*'",
    "lint:fix": "npm run lint -- --fix",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:css:fix": "npm run lint:css -- --fix",
    "refresh": "curl -X POST http://localhost:4000/__refresh",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "sb": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "chromatic --exit-zero-on-changes"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.5",
    "gatsby": "^3.14.5",
    "gatsby-plugin-advanced-sitemap": "^2.0.0",
    "gatsby-plugin-feed": "^3.14.0",
    "gatsby-plugin-google-tagmanager": "^3.14.0",
    "gatsby-plugin-image": "^1.14.2",
    "gatsby-plugin-manifest": "^3.14.0",
    "gatsby-plugin-mdx": "^2.14.0",
    "gatsby-plugin-postcss": "^4.14.0",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-sharp": "^3.14.3",
    "gatsby-remark-autolink-headers": "^4.11.0",
    "gatsby-remark-images": "^5.11.0",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-transformer-remark": "^4.11.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "postcss": "^8.3.11",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-calendly": "^2.2.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-docs": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.3.12",
    "@storybook/manager-webpack5": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "autoprefixer": "^10.4.0",
    "axios": "^0.24.0",
    "babel-loader": "^8.2.3",
    "chromatic": "^5.10.2",
    "cypress": "^8.7.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "gatsby-plugin-page-progress": "^2.2.1",
    "gh-pages": "^3.2.3",
    "react-spring": "^9.3.0",
    "storybook-addon-gatsby": "^0.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "tailwindcss": "^2.2.19",
    "xml2js": "^0.4.23"
  }
}
